<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
       <th>id</th>
       <th>created_at</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order|%>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Order', new_order_path %>
